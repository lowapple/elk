Log:
  Level: DEBUG              # log level: DEBUG, INFO, WARN, ERROR, FATAL
  WriteFile: true           # 파일출력여부 true:콘솔,파일출력, false:콘솔만 출력,docker 일때 유용함
  Path: logs
  FileName: app.log
  RotatePattern: "%Y%m%d"   # rotate 파일명 규칙 %Y%m%d:20220506, %Y%m%d%H%M:202205061657
  RotateMaxAge: 60          # (days,일) rotate 파일 보관기간

